    <decisions>
        <name>Is_Latest_Version_the_Active_Version</name>
        <label>Is Latest Version the Active Version?</label>
        <locationX>418</locationX>
        <locationY>398</locationY>
        <defaultConnector>
            <targetReference>Get_Latest_Flow_Version</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Latest is NOT Active (Default)</defaultConnectorLabel>
        <rules>
            <name>Latest_is_Active</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Flow_Definitions.ActiveVersionId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>Flow_Definitions.LatestVersionId</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Flow_Definitions</targetReference>
            </connector>
            <label>Latest is Active</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_there_an_Active_Version</name>
        <label>Is there an Active Version?</label>
        <locationX>572</locationX>
        <locationY>638</locationY>
        <defaultConnector>
            <targetReference>Add_to_Collection</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Latest is NOT Active (Default)</defaultConnectorLabel>
        <rules>
            <name>No_Active_Version</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Flow_Definitions.ActiveVersionId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Add_to_No_Active_Collection</targetReference>
            </connector>
            <label>No Active Version</label>
        </rules>
    </decisions>
    <description>Check flows and lists those with an Active Version that is not the Latest Version and lists those with no Active Version.</description>
    <formulas>
        <name>fCRLF</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE({!ttCRLF}, &quot;~&quot;, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>fFlowEntry</name>
        <dataType>String</dataType>
        <expression>LPAD( TEXT( {!Flow_Definitions.VersionNumber}) , 5, CHR(0160) ) &amp; LPAD( TEXT( {!Get_Latest_Flow_Version.VersionNumber} ), IF( {!Flow_Definitions.VersionNumber} &gt; 99, 7, IF( {!Flow_Definitions.VersionNumber} &gt; 9, 8, 9 ) ), CHR(0160)  ) &amp; LPAD( CHR(0160), IF( {!Get_Latest_Flow_Version.VersionNumber} &gt; 99, 4, IF( {!Get_Latest_Flow_Version.VersionNumber} &gt; 9, 5, 6 ) ), CHR(0160) ) &amp; {!Flow_Definitions.Label} &amp; {!fCRLF}</expression>
    </formulas>
    <formulas>
        <name>fFlowEntryNoActive</name>
        <dataType>String</dataType>
        <expression>LPAD( &quot;--&quot; , 5, CHR(0160) ) &amp; LPAD( TEXT( {!Get_Latest_Flow_Version.VersionNumber} ), 9, CHR(0160) ) &amp; LPAD( CHR(0160), IF( {!Get_Latest_Flow_Version.VersionNumber} &gt; 99, 4, IF( {!Get_Latest_Flow_Version.VersionNumber} &gt; 9, 5, 6 ) ), CHR(0160) ) &amp; {!Flow_Definitions.Label} &amp; {!fCRLF}</expression>
    </formulas>
