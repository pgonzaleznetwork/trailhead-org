<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Github_URL_must_be_valid</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(ISBLANK(Git_Pull_Request_URL__c ))  

&amp;&amp; 


NOT(BEGINS(Git_Pull_Request_URL__c, &quot;https://github.com/websummit/sfdc/pull/new/&quot;))</errorConditionFormula>
    <errorDisplayField>Git_Pull_Request_URL__c</errorDisplayField>
    <errorMessage>This is not a valid Github Pull Request URL.</errorMessage>
</ValidationRule>
